{
    "$schema": "http://json.schemastore.org/template",
    "author": "Net Dev",
    "classifications": [ "Common", "Clean Architecture", "Domain Drive Design" ],
    "identity": "Nett.Templates",
    "name": "Nett Dev Templates",
    "shortName": "nett.dbcontext",
    "symbols": {
      "dbcontextName":{
        "type": "parameter",
        "description": "The name of dbcontext code file and class.",
        "datatype": "text",
        "replaces": "AppDbContext",
        "fileRename": "AppDbContext",
        "defaultValue": "AppDbContext"
      },
      "namespace": {
        "type": "parameter",
        "description": "Namespace for the generated code",
        "replaces": "NettDev.Namespace"
      }
    }
  }
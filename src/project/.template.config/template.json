{
    "$schema": "http://json.schemastore.org/template",
    "author": "Jeziel Moura",
    "classifications": [ "App", "Project" ],
    "identity": "Nett.Project",
    "name": "Nett Project Template",
    "shortName": "nett.project",
    "tags": {
      "type": "project"
    },
    "symbols": {
      "nameOverride":{
        "type": "derived",
        "valueSource": "name",
        "valueTransform": "removeSpaces",
        "replaces": "NameOverride",
        "fileRename": "NameOverride",
        "defaultValue": "NameOverride"
      }
    },
    "forms": {
      "removeSpaces": {
        "identifier": "replace",
        "pattern": "\\ ",
        "replacement": ""
      }
    }
}

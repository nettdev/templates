{
    "$schema": "http://json.schemastore.org/template",
    "author": "Net Dev",
    "classifications": [ "Common", "Clean Architecture", "Domain Drive Design" ],
    "identity": "Nett.Templates",
    "name": "Nett Dev Templates",
    "shortName": "nett.repository",
    "symbols": {
      "aggregateName":{
        "type": "parameter",
        "description": "The name of Aggregate code file and class.",
        "datatype": "text",
        "replaces": "Aggregate",
        "fileRename": "Aggregate",
        "defaultValue": "Aggregate"
      },
      "namespace": {
        "type": "parameter",
        "description": "Namespace for the generated code",
        "replaces": "NettDev.Namespace"
      }
    }
  }